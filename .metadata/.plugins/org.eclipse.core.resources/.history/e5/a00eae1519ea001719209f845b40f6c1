package com.sapient.vo;

public class Emp {
	private int eid;
	private String ename;
	private int sal;
	


	public Emp(int eid, String ename, int sal) {
		super();
		this.eid = eid;
		this.ename = ename;
		this.sal = sal;
	}
	
	public void setSal(int sal) {
		this.sal = sal;
	}

	public int getEid() {
		return eid;
	}

	public String getEname() {
		return ename;
	}

	public int getSal() {
		return sal;
	}
	

}
